{
  "name": "ts-errors",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "run:script": "tsx src/index.ts",
    "server": "node server/server.js",
    "watch:server": "nodemon  -i ./src -e mjs,json ./server/server.mjs",
    "codegen": "openapi-typescript spec.yml  -o src/api.ts"
  },
  "devDependencies": {
    "nodemon": "3.1.10",
    "openapi-typescript": "7.7.1",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "express": "5.1.0"
  }
}